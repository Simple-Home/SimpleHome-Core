{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "authlete/authlete-laravel": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "fx3costa/laravelchartjs": "^2.8",
        "guzzlehttp/guzzle": "^7.0.1",
        "joshbrw/laravel-module-installer": "^2.0",
        "kris/laravel-form-builder": "^1.40",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "nanigans/single-table-inheritance": "~1.0",
        "nwidart/laravel-modules": "^8.0",
        "pavel-mironchik/laravel-backup-panel": "^2.1",
        "php-mqtt/client": "^1.1",
        "pusher/pusher-php-server": "^6.0",
        "rachidlaasri/laravel-installer": "^4.1",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/laravel-backup": "^6.16",
        "ycs77/laravel-form-builder-bs4": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracasts/cypress": "^1.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "^9.3.3",
        "reliese/laravel": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "exclude-from-classmap": [
            "vendor\\laravel\\framework\\src\\Illuminate\\Routing\\CompiledRouteCollection.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "echo @php artisan package:discover --ansi",
            "echo @php artisan migrate -n --force",
            "npm install --save",
            "npm install run production"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
